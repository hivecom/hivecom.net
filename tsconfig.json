{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "target": "ES2017",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true
  },
  "paths": {
    "@/components/*": [
      "components/*"
    ],
    "@/types/*": [
      "types/*"
    ]
  }
}
